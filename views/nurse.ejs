<!DOCTYPE html>
<html>
  <head>
    <title>Nurse</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table>
      <tr>
        <td>
          <% studentsList.forEach(student => {%>
          <div class="user">
            <div class="name">
              <%= student.name %> <%= student.surname %>  
            </div>
            <div>
              <span class="birthDate"><%= student.birthDate %> </span>
            </div> 
            <div>
              <%= student.classNumber %>
            </div>
            <div class="create">
              <a href="/medcard/<%= student._id %>" class="btn btn3">Update</a>
            </div>
          </div> 
          <%})%>
        </td>
      </tr>
    </table>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.24.0/axios.js"></script>
    <script src="/js/main.js"></script>
  </body>
</html>
